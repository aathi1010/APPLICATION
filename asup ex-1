        "def is_prime(n):\n",
        "    if n <= 1:\n",
        "        return False\n",
        "    for i in range(2, int(n**0.5)+1):\n",
        "        if n % i == 0:\n",
        "            return False\n",
        "    return True\n",
        "\n",
        "n = 5\n",
        "primes = []\n",
        "i = 2\n",
        "while len(primes) < n:\n",
        "    if is_prime(i):\n",
        "        primes.append(i)\n",
        "    i += 1\n",
        "print(\"First\", n, \"primes:\", primes)\n"
      ]
    },
