        "import math\n",
        "# Input two numbers\n",
        "a = int(input(\"Enter first number: \"))\n",
        "b = int(input(\"Enter second number: \")) # Using built-in gcd() function\n",
        "gcd = math.gcd(a, b)\n",
        "print(\"The GCD is\", gcd)\n"
